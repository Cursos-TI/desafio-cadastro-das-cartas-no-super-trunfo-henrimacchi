#include <stdio.h>

int main (){

int codigo de cidade;
char nome [25];
int populacao;
float area;
double PIB;
int pontos turisticos;

scanf("%d", &cidade);
printf("cidade: "%d", cidade);

scanf("%s", &nome[25]);
printf("nome: "%d", nome);

scanf("%d", &populacao);
printf("populacao: "%d", populacao);

scanf("%f", &area);
printf("area: "%f", area);

scanf("%f", &PIB);
printf("PIN "%f", PIB);

scanf("%d", &pontos turisticos);
printf("pontos turisticos "%d", pontos turisticos);

return 0;

}